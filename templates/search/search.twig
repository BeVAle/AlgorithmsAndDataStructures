{% extends 'base.html.twig' %}

{% block main %}
    <div class="form-group">
        <label class="control-label required" for="post_title">Поиск рабина</label>
        <div class="row">
            <div class="col-md-4">
                <input type="text" id="rabinSearch_text" required="required" class="form-control"placeholder="Какие данные">
            </div>
            <div class="col-md-2">
                <input type="text" id="rabinSearch_pattern" required="required" class="form-control"  placeholder="Че ищем?">
            </div>
            <div class="col-md-3">
                <button type="submit" class="btn btn-primary rabinSearch_search">Search</button>
            </div>
            <div class="col-md-3">
                <input type="text" id="rabinSearch_finish" class="form-control" value="" disabled>
            </div>
        </div>
    </div>

    <div class="form-group">
        <label class="control-label required" for="post_title">Поиск Бойера - Мура</label>
